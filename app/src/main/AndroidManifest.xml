<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sevenlogics.babynursing">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BabyDetailActivity"
            android:theme="@style/AppTheme.NoActionBar"
            >
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <activity
            android:name=".ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".NursingActivity"
            android:label="@string/title_nursing_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity
            android:name=".NursingDetailActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PumpingActivity"
            android:label="Pumping"
            android:theme="@style/PumpingTheme"/>
        <activity
            android:name=".PumpingDetailActivity"
            android:label="New Pumping"
            android:theme="@style/PumpingTheme"/>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>